{
  "name": "bingobutler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "discord": "nodemon --exec ts-node discord/index.ts",
    "api": "nodemon --exec ts-node api/index.ts",
    "apibuild": "rm -rf build && tsc --noEmit false",
    "pm2": "NODE_ENV=production pm2 delete bingobot; pm2 start build/api/index.js --name bingobot",
    "prod": "NODE_ENV=production node build/api/index.js",
    "prodwin": "set NODE_ENV=production && node build/api/index.js"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.1.1",
    "next": "11.1.2",
    "node-fetch": "^2.0.0",
    "nodemon": "^2.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "4.4.2"
  }
}
